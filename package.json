  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@svgr/rollup": "^6.3.1",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.26",
    "fs-extra": "^11.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.33.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "prepublish": "yarn run build",
    "clean": "rm -rf dist",
    "generate": "rm -rf src/baseIcons src/componentIcons src/doctypeIcons src/illustrativeIcons src/logoIcons && yarn node createComponents",
    "build": "yarn run clean && rollup -c && yarn run frank && yarn run copytypes",
    "frank": "yarn node frank.js",
    "copytypes": "yarn node copyTypes.js && rm -rf dist/types"
  },
  "peerDependencies": {
    "react": ">=16.2.0",
    "styled-components": ">=5.3.6"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  }
